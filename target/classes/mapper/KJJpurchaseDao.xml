<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.springlec.base.dao.KJJpurchaseDao">

<select id="product" resultType="com.springlec.base.model.KJJpurchaseDto">
	SELECT * FROM product WHERE pid = #{pid}
</select>

<select id="customer" resultType="com.springlec.base.model.KJJpurchaseDto">
	SELECT * FROM customer WHERE cid = #{cid}
</select>





</mapper>